{
  "name": "telescope",
  "author": "Sal Costa <costas0811@hotmail.com> (http://anerrantprogrammer.com",
  "displayName": "Telescope",
  "description": "",
  "version": "0.0.4",
  "license": "MIT",
  "publisher": "anerrantprogrammer",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnErrantProgrammer/telescope"
  },
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [{
      "command": "telescope.createSession",
      "title": "ðŸ”­ Create Session"
    }, {
      "command": "telescope.joinSession",
      "title": "ðŸ”­ Join Session"
    }, {
      "command": "telescope.leaveSession",
      "title": "ðŸ”­ Leave Session"
    }, {
      "command": "telescope.showSession",
      "title": "ðŸ”­ Show Session"
    }, {
      "command": "telescope.linkSession",
      "title": "ðŸ”­ Show Session Link"
    }, {
      "command": "telescope.addDocument",
      "title": "ðŸ”­ Add Document to Session"
    }, {
      "command": "telescope.changeName",
      "title": "ðŸ”­ Change User Name"
    }, {
      "command": "telescope.changeHost",
      "title": "ðŸ”­ Change Host"
    }, {
      "command": "telescope.connectToHost",
      "title": "ðŸ”­ Connect To Host"
    }, {
      "command": "telescope.disconnectFromHost",
      "title": "ðŸ”­ Disconnect From Host"
    }],
    "menus": {
        "editor/context": [{
            "command": "telescope.addDocument",
            "group": "1_modification"
        }]
    },
    "configuration": {
      "type": "object",
      "title": "Telescope Settings",
      "properties": {
        "telescope.username": {
          "type": "string",
          "default": "HamCutter",
          "description": "User Display Name"
        },
        "telescope.host": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "CodePulse Server URL"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "eslint": "^4.6.1",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "diff": "^3.4.0",
    "socket.io-client": "^2.0.4",
    "socketio": "^1.0.0",
    "tmp":"^0.0.33"
  }
}